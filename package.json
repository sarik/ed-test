{
  "name": "container-app",
  "version": "1.0.0",
  "description": "Container app that builds the foundation of multiple SPAs",
  "scripts": {
    "compile:sass": "node-sass ./src/sass/main.scss ./src/assets/css/styles.css -w",
    "watch": "webpack-dev-server --port 8233",
    "build": "webpack -p --progress",
    "install:portal": "npm i",
    "install:attendance": "cd ../ed-attendance-web-app/ && npm i",
    "install:videoconferencing": "cd ../ed-videoconferencing-web-app/ && npm i",
    "install:home": "cd ../ed-home-web-app/ && npm i",
    "install:user-settings": "cd ../ed-user-settings-web-app/ && npm i",
    "install:discovery": "cd ../ed-discovery-web-app/ && npm i",
    "install-all": "npm run install:portal && npm run install:attendance && npm run install:home && npm run install:user-settings && npm run install:discovery && npm run install:videoconferencing",
    "start:portal": "npm run watch",
    "start:attendance": "cd ../ed-attendance-web-app/ && npm run watch",
    "start:home": "cd ../ed-home-web-app/ && npm run watch",
    "start:discovery": "cd ../ed-discovery-web-app/ && npm run watch",
    "start:videoconferencing": "cd ../ed-videoconferencing-web-app/ && npm run watch",
    "start:user-settings": "cd ../ed-user-settings-web-app/ && npm run watch",
    "start-all": "concurrently --kill-others --kill-others-on-fail -p name --names \"portal,attendance,home,user-settings\" \"npm run start:portal\" \"npm run start:attendance\" \"npm run start:home\" \"npm run start:user-settings\" \"npm run start:discovery\" \"npm run start:videoconferencing\"",
    "pull:portal": "git pull",
    "pull:attendance": "cd ../ed-attendance-web-app/ && git pull",
    "pull:home": "cd ../ed-home-web-app/ && git pull",
    "pull:user-settings": "cd ../ed-user-settings-web-app/ && git pull",
    "pull-all": "npm run pull:portal && npm run pull:attendance && npm run pull:home && npm run pull:user-settings",
    "analyze": "source-map-explorer release/main.*"
  },
  "author": "Abhishek Bansal",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "source-map-explorer": "^2.4.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "@zoomus/websdk": "^1.7.0",
    "bulma": "^0.8.0",
    "concurrently": "^4.1.1",
    "copy-webpack-plugin": "^4.5.1",
    "intl-messageformat": "^6.0.3",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "node-sass": "^7.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-flickity-component": "^3.4.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "single-spa": "^4.0.0",
    "systemjs": "^0.21.4"
  }
}
